{
 "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
  "sourceMap": true,
  "baseUrl": "AngularApp",
  "paths": {
    "services/*": [ "app/user/src/*" ],
    "pages/*": [ "app/user/html/*"]
  },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
  "lib": [ "es2015", "dom" ],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "typeRoots": [
      "node_modules/@types/",
      "webpack-env"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "bin"
  ]
}
